{
  "name": "flownote",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"yarn dev:client\" \"yarn dev:server\"",
    "dev:client": "vite",
    "dev:server": "nodemon --exec \"vite-node src/server/index.ts\" --watch src/server --ext ts --delay 1",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@notionhq/client": "^2.3.0",
    "@tailwindcss/vite": "^4.1.0",
    "@types/cors": "^2.8.17",
    "axios": "^1.8.4",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "express": "4.19.2",
    "openai": "^4.91.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "6.22.3",
    "tailwindcss": "^4.1.0",
    "uuid": "^11.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/express": "4.17.21",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "nodemon": "^3.1.9",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-node": "2.0.4",
    "vite-plugin-svgr": "^4.3.0"
  }
}
